{
  "name": "@dataprism/plugin-ironcalc-formula",
  "version": "0.1.0",
  "description": "Excel-compatible formula engine plugin for DataPrism powered by IronCalc",
  "type": "module",
  "main": "./dist/ironcalc-plugin.js",
  "types": "./dist/ironcalc-plugin.d.ts",
  "files": [
    "dist/",
    "pkg/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "wasm-pack build --target web --out-dir pkg --release",
    "build:ts": "tsc && vite build",
    "dev": "npm run build:wasm && vite dev",
    "test": "npm run test:rust && npm run test:integration",
    "test:rust": "wasm-pack test --chrome --headless",
    "test:integration": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint ts/**/*.ts",
    "clean": "rm -rf dist pkg target",
    "benchmark": "node tests/benchmark.js"
  },
  "dependencies": {
    "happy-dom": "^18.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.2.0",
    "vite": "^5.4.8",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "dataprism",
    "plugin",
    "formula",
    "excel",
    "spreadsheet",
    "calculation",
    "wasm",
    "ironcalc"
  ],
  "author": "DataPrism Team",
  "license": "MIT",
  "homepage": "https://github.com/srnarasim/dataprism-plugins",
  "repository": {
    "type": "git",
    "url": "https://github.com/srnarasim/dataprism-plugins.git",
    "directory": "packages/ironcalc-formula"
  }
}
